#include <stdio.h>
int main() {
    int n, i, flag = 0;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n < 0) {
        printf("Enter positive number.\n");
    } 
    else {
        for (i = 1; i <= n; i++) {
            if (i * i == n) {
                flag = 1;   
                break;
            }
        }

        if (flag == 1) {
            printf("Perfect square.\n");
        } else {
            printf("Not a perfect square.\n");
        }
    }

    return 0;
}
