#include <stdio.h>
int main(){
    int n,og,product=1;
    printf("Enter a number: ");
    scanf("%d", &n);
    og = n;
    if (n < 0){
        n = -n;
    }
    if (n == 0){
        product=0;
    }
    else {
        while (n > 0){
            product = product * (n%10);
            n /= 10;
        }
    }
    printf("product of digits of %d is: %d", og, product);
    return 0;
}
