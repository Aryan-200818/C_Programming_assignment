#include <stdio.h>
int main(){
    int n;
    printf("Enter a number: ");
    scanf("%d",&n);
    if(n%5==0 && n%11==0){
        printf("Entered number is divisible by both 5 and 11.\n");
    }
    else if(n%5==0 && n%11!=0){
        printf("Entered number is divisible by 5 but not by 11.\n");
    }
    else if(n%5!=0 && n%11==0){
        printf("Entered number is divisible by 11 but not 5.\n");
    }
    else{
        printf("Entered number is not divisible by both 5 and 11.\n");
    }
    return 0;
} 
