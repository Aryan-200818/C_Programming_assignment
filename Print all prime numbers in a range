#include <stdio.h>
int is_prime(int n){
    if(n<2) return 0;
    for(int i=2;i<=n/2;i++){
        if (n%i == 0)
            return 0;
    }
    return 1;
}
int main(){
    int start,end;
    printf("Enter starting number: ");
    scanf("%d",&start);
    printf("Enter end number: ");
    scanf("%d",&end);
    printf("Prime number between %d and %d are: \n",start,end);
    for(int i=start;i<=end;i++){
        if(is_prime(i)){
            printf("%d\n",i);
        }
    }
    return 0;
}
