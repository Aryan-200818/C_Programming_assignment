#include <stdio.h>
int main(){
    int num;
    printf("Enter a number: ");
    scanf("%d",&num); // number input from user
    
    // checking weather a number is even or odd
    if (num%2 == 0){
        printf("The entered number is even.\n");
    }
    else{
        printf("The entered number is odd.\n");
    }
    return 0;
}
