#include <stdio.h>
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n % 3 == 0 && n % 7 == 0) {
        printf("%d is divisible by both 3 and 7.\n", n);
    } 
    else if (n % 3 == 0) {
        printf("%d is divisible by only 3.\n", n);
    } 
    else if (n % 7 == 0) {
        printf("%d is divisible by only 7.\n", n);
    } 
    else {
        printf("%d is not divisible by 3 or 7.\n", n);
    }

    return 0;
}
