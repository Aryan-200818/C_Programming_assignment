#include <stdio.h>
int main(){
    int n,i,is_prime=1;
    printf("Enter a number: ");
    scanf("%d",&n);
    if (n<=1){
        printf("Enter a valid number (greater than 1).\n");
    }
    else{
        for(i=2;i<=n/2;i++){
            if (n%i==0){
                is_prime=0;
                break;
            }
        }
        if(is_prime==0) printf("%d is a composite number.\n",n);
        else printf("%d is a prime number.\n",n);
    }

    return 0;
} 
